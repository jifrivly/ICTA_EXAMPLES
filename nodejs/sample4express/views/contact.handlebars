<body class="">

    <div class="container my-1">
        <div class="row">
            <div class="col col-12 col-sm-12 col-md-2 col-lg-3 col-xl-3"></div>
            <div class="col col-12 col-sm-12 col-md-8 col-lg-6 col-xl-6 border rounded bg-secondary p-4">
                <form action="/getdata" method="post">
                    <div class="form-group">
                        <label for="name">Name : </label>
                        <input type="text" class="form-control" name="name" id="name" aria-describedby="helpId"
                            placeholder="">
                        <small id="helpId" class="form-text text-muted">Help text</small>
                    </div>
                    <div class="form-group">
                        <label for="address">Your Address : </label>
                        <textarea class="form-control" name="address" id="address" rows="3"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="place">Place : </label>
                        <input type="text" class="form-control" name="place" id="place" aria-describedby="helpId"
                            placeholder="">
                        <small id="helpId" class="form-text text-muted">Help text</small>
                    </div>
                    <div class="form-group">
                        <label for="phone">Phone : </label>
                        <input type="tel" class="form-control" name="phone" id="phone" aria-describedby="helpId"
                            placeholder="">
                        <small id="helpId" class="form-text text-muted">Help text</small>
                    </div>
                    <div class="form-group">
                        <label for="email">Email ID : </label>
                        <input type="email" class="form-control" name="email" id="email" aria-describedby="emailHelpId"
                            placeholder="">
                        <small id="emailHelpId" class="form-text text-muted">Help text</small>
                    </div>
                    <div class="form-group">
                        <label for="message">Your message : </label>
                        <textarea class="form-control" name="message" id="message" rows="3"></textarea>
                    </div>
                    <div class="text-right">
                        <button type="submit" class="btn btn-primary">Send <i class="far fa-paper-plane"></i> </button>
                    </div>
                </form>
            </div>
            <div class="col col-12 col-sm-12 col-md-2 col-lg-3 col-xl-3"></div>
        </div>
    </div>

</body>

</html>