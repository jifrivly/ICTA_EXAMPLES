<div class="jumbotron">
    <h1 class="display-3">About Page</h1>
    <p class="lead">Jumbo helper text</p>
    <hr class="my-2">
    <p>More info</p>
    <p class="lead">
        <a class="btn btn-primary btn-lg" href="" role="button">Jumbo action name</a>
    </p>
    <table class="table table-dark">
        <thead>
            <tr>
                <th> No. </th>
                <th> Contents </th>
                <th> Datas </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td scope="row">01</td>
                <td> Name </td>
                <td>{{name}}</td>
            </tr>
            <tr>
                <td scope="row"> 02 </td>
                <td> Address </td>
                <td>{{address}} </td>
            </tr>
            <tr>
                <td scope="row"> 03 </td>
                <td> Place </td>
                <td>{{place}} </td>
            </tr>
            <tr>
                <td scope="row"> 04 </td>
                <td> Phone </td>
                <td>{{phone}} </td>
            </tr>
            <tr>
                <td scope="row"> 05 </td>
                <td> Email </td>
                <td>{{email}} </td>
            </tr>
            <tr>
                <td scope="row"> 06 </td>
                <td> Message </td>
                <td>{{message}} </td>
            </tr>
        </tbody>
    </table>
</div>